<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.student.telefony.EdycjaActivity">
    <TableRow>
        <TextView
            android:id="@+id/producent_etykieta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Producent"
            android:layout_weight="1"
            />
        <EditText
            android:id="@+id/producent"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_weight="2"/>
    </TableRow>
    <TableRow>
        <TextView
            android:id="@+id/model_etykieta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Model"
            android:layout_weight="1"
            />
        <EditText
            android:id="@+id/model"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_weight="2"/>
    </TableRow>
    <TableRow>
        <TextView
            android:id="@+id/wersja_etykieta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Wersja androida"
            android:layout_weight="1"
            />
        <EditText
            android:id="@+id/wersja"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_weight="2"/>
    </TableRow>
    <TableRow>
        <TextView
            android:id="@+id/www_etykieta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="WWW"
            android:layout_weight="1"
            />
        <EditText
            android:id="@+id/www"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_weight="2"/>
    </TableRow>
    <TableRow>
        <Button
            android:layout_weight="1"
            android:id="@+id/strona"
            android:text="WWW"
            />
        <Button
            android:layout_weight="1"
            android:id="@+id/anuluj"
            android:text="Anuluj"
            />
        <Button
            android:layout_weight="1"
            android:id="@+id/zapisz"
            android:text="Zapisz"
            />
    </TableRow>

</TableLayout>
